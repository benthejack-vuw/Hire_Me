{
  "name": "hello_field",
  "version": "0.0.1",
  "description": "I'd love to work with you FIELD, your work is inspiring! Here's a little introduction to myself.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.dev.js --mode=development -d",
    "build": "run-s mkdirs compile",
    "compile": "run-p pack copy-web-files",
    "pack": "webpack --mode=production",
    "mkdirs": "mkdirp ./www && mkdirp ./www/assets && mkdirp ./www/scripts",
    "copy-web-files": "cpx 'web_files/assets/*.*' 'www/assets' && cpx ./web_files/index.css ./www && cpx ./web_files/index.html ./www && cpx ./web_files/favicon.png ./www"
  },
  "author": "Ben Jack",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0-beta.6",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "cpx": "^1.5.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-preset-env": "^1.7.0",
    "three": "0.103.0"
  }
}
